{
	"parser": "typescript",
	"singleQuote": false,
	"useTabs": true,
	"bracketSpacing": true,
	"jsxBracketSameLine": false,
	"arrowParens": "always",
	"trailingComma": "es5",
	"printWidth": 120,
	"endOfLine": "lf",
	"tabWidth": 4,
	"semi": true,
	"singleAttributePerLine":false,
	"overrides": [
		{
		  "files": ["*.json", "*.jsonc", "*.json5"],
		  "options": {
			"parser": "json5",
			"quoteProps": "preserve",
			"trailingComma": "none"
		  }
		}
	  ]
	
}
